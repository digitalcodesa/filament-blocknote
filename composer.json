{
    "name": "digitalcodesa/filament-blocknote",
    "description": "A Laravel Filament package to add the BlockNote field to your forms.",
    "keywords": [
        "laravel",
        "filament-blocknote"
    ],
    "homepage": "https://github.com/digitalcodesa/filament-blocknote",
    "support": {
        "issues": "https://github.com/digitalcodesa/filament-blocknote/issues",
        "source": "https://github.com/digitalcodesa/filament-blocknote"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Abdo Sh",
            "email": "info@digitalcode.sa",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.2.122",
        "spatie/laravel-package-tools": "^1.16.5",
        "illuminate/contracts": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.18.1"
    },
    "autoload": {
        "psr-4": {
            "Digitalcode\\FilamentBlocknote\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digitalcode\\FilamentBlocknote\\FilamentBlocknoteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
